<h1 class='mt-2'>Etiquetas</h1>

<%= render 'filter' %>

<table class="table table-bordered">
  <thead class="thead-light">
  <tr>
  <th>Nombre</th>
  <th>Etiquetas</th>
  <th class='text-right'><%= link_to 'Nueva', new_backend_tag_path, class: 'btn btn-primary' %></th>
</tr>
  </thead>
  <tbody id='tags'>
    <%= render @presenter.tags %>
  </tbody>
</table>

<%= will_paginate @presenter.tags %>