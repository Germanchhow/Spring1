<h1 class='mt-2'>Categorias</h1>

<%= render 'filter' %>

<table class="table table-bordered">
  <thead class="thead-light">
   <tr>
      <th>Nombre</th>
      <th>Productos</th>
      <th class='text-right'><%= link_to 'Nueva', new_backend_category_path, class: 'btn btn-primary' %></th>
    </tr>
   
  </thead>
  <tbody id='categories'>
    <%= render @presenter.categories %>
  </tbody>
</table>

<%= will_paginate @presenter.categories %>